<main role="main">

  <!-- Main jumbotron for a primary marketing message or call to action -->
  <div class="jumbotron">
    <div class="container">
      <h1 class="display-3">Cookies list</h1>
      
      <%
      	Cookie cookie = null;
		Cookie[] cookies =null;
		  
		cookies = request.getCookies();
		
		if(cookies != null){
			for(Cookie c : cookies){
				out.print("<hr>");
				out.print("<b> Name : </b> "+ c.getName() );
				out.print("<b> Value : </b> "+ c.getValue() + "<br/> </hr>" );
			}
		}
		else{
			out.println("<h2>Aucun cookie trouvé</h2>");
		}
		
		
      
      %>
    
      
    <div class="jumbtron">
      <form action="http://localhost:8080/test/servletcookies" method="POST" >
		<label for="cname">Nom cookie</label><br>
	  	<input type="text" id="cname" name="cname"><br>
	  	<label for="cvalue">Valeur </label><br>
	  	<input type="text" id="cvalue" name="cvalue">
	  	
		<input type="submit" value="Créer un nouveau cookie">
	</form>
    </div>
  </div>

